{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "order"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "market": {
      "model": "market",
      "via": "orders"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "orders"
    },
    "side": {
      "type": "enumeration",
      "enum": [
        "BUY",
        "SELL"
      ]
    },
    "payment": {
      "model": "payment",
      "via": "orders"
    },
    "public_key": {
      "type": "string"
    },
    "max_supply": {
      "type": "float"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "ACTIVE",
        "INACTIVE"
      ]
    },
    "active_trade": {
      "type": "integer"
    },
    "price": {
      "type": "float"
    },
    "min_qty": {
      "type": "float"
    },
    "max_qty": {
      "type": "float"
    },
    "qty": {
      "type": "float"
    },
    "open_qty": {
      "type": "float"
    },
    "note": {
      "type": "string"
    },
    "created_at": {
      "type": "datetime"
    },
    "updated_at": {
      "type": "datetime"
    }
  }
}
