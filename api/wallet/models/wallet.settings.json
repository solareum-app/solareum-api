{
  "kind": "collectionType",
  "collectionName": "wallets",
  "info": {
    "name": "wallet"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "wallets"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "p2p",
        "self_custody"
      ]
    },
    "network": {
      "type": "enumeration",
      "enum": [
        "SPL",
        "ERC20",
        "TRC20"
      ]
    },
    "public_key": {
      "type": "string"
    },
    "hash_key": {
      "type": "string"
    },
    "created_at": {
      "type": "datetime"
    },
    "updated_at": {
      "type": "datetime"
    }
  }
}
