{
  "apps": [
    {
      "exec_mode": "fork_mode",
      "name": "stg-solareum-api",
      "script": "node_modules/strapi/bin/strapi.js",
      "instances": 2,
      "args": "start",
      "watch": false,
      "ignore_watch": [
        "node_modules",
        "log"
      ],
      "watch_options": {
        "followSymlinks": false
      },
      "env_production": {
        "NODE_ENV": "production",
        "PORT": 6610,
        "HOST_PATH": "api-stg.solareum.app"
      }
    }
  ]
}
