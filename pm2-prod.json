{
  "apps": [
    {
      "exec_mode": "fork_mode",
      "name": "prd-solareum-api",
      "script": "node_modules/strapi/bin/strapi.js",
      "instances": "max",
      "args": "start",
      "watch": false,
      "ignore_watch": [
        "node_modules",
        "log"
      ],
      "watch_options": {
        "followSymlinks": false
      },
      "env_production": {
        "NODE_ENV": "production",
        "PORT": 6600,
        "HOST_PATH": "api.solareum.app"
      }
    }
  ]
}
